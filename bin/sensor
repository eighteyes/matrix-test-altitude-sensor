#!/usr/bin/env node

var os = require( 'os' );

var height = Math.round( Math.random() * 100 ) * 100;

setInterval( function () {
  height += Math.round( Math.random() * 100 ) - 50;
  process.stdout.write( JSON.stringify( {
    type: 'altitude',
    data: {
      'value': height
    }
  }));
}, process.env.MATRIX_TEST_SENSOR_REFRESH_RATE || 5000 );
